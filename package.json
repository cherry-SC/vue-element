{
  "name": "sc-element",
  "version": "1.0.2",
  "description": "A Vue 3 component library",
  "type": "module",
  "author": "sc",
  "license": "MIT",
  "keywords": [
    "vue3",
    "component",
    "library",
    "UI"
  ],
  "homepage": "https://github.com/cherry-SC",
  "repository": {
    "type": "git",
    "url": "https://github.com/cherry-SC"
  },
  "files": [
    "dist"
  ],
  "sideEffects": [
    "dist/index.css"
  ],
  "main": "./dist/umd/sc-element.umd.cjs",
  "module": "./dist/es/sc-element.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/es/sc-element.js",
      "require": "./dist/umd/sc-element.umd.cjs"
    },
    "./dist/": {
      "import": "./dist/",
      "require": "./dist/"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "npm run build-only && npm run move-style",
    "preview": "vite preview",
    "build-only": "run-p build-umd build-es",
    "build-umd": "vite build --config vite.umd.config.ts",
    "build-es": "vite build --config vite.es.config.ts",
    "move-style": "move-file dist/es/index.css dist/index.css",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix --cache",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@floating-ui/vue": "^1.1.10",
    "@fortawesome/fontawesome-svg-core": "^7.1.0",
    "@fortawesome/free-solid-svg-icons": "^7.1.0",
    "@fortawesome/vue-fontawesome": "^3.1.3",
    "@popperjs/core": "^2.11.8",
    "async-validator": "^4.2.5",
    "lodash-es": "^4.17.23",
    "vite-plugin-eslint": "^1.8.1",
    "vitest": "^4.0.18"
  },
  "peerDependencies": {
    "vue": "^3.5.26"
  },
  "devDependencies": {
    "@tsconfig/node24": "^24.0.4",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^24.10.9",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vitejs/plugin-vue-jsx": "^5.1.3",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.39.2",
    "eslint-plugin-vue": "~10.7.0",
    "jiti": "^2.6.1",
    "jsdom": "^27.4.0",
    "move-file-cli": "^3.0.0",
    "npm-run-all2": "^8.0.4",
    "postcss-advanced-variables": "^5.0.0",
    "postcss-color-mix": "^1.1.0",
    "postcss-each": "^1.1.0",
    "postcss-for": "^2.1.1",
    "postcss-nested": "^7.0.2",
    "typescript": "~5.9.3",
    "vite": "^7.3.1",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-vue-devtools": "^8.0.5",
    "vitepress": "^2.0.0-alpha.16",
    "vue": "^3.5.26",
    "vue-tsc": "^3.2.2"
  },
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  }
}